<Window x:Class="JSON_ex.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JSON_ex"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label Content="Select Character" HorizontalAlignment="Left" Margin="53,50,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="cboCharacter" HorizontalAlignment="Left" Margin="41,76,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="cboCharacter_SelectionChanged"/>
        <Label x:Name="lblname" Content="Label" HorizontalAlignment="Left" Margin="436,10,0,0" VerticalAlignment="Top" Height="106" Width="364" FontSize="24"/>
        <Image x:Name="imgCharacter" HorizontalAlignment="Left" Height="264" Margin="233,87,0,0" VerticalAlignment="Top" Width="305"/>


    </Grid>
</Window>
